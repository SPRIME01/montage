{
  "name": "montage",
  "version": "0.14.14",
  "description": "Build your next application with a browser based platform that really gets the web.",
  "repository": {
    "type": "git",
    "url": "https://github.com/montagejs/montage.git"
  },
  "main": "montage",
  "overlay": {
    "browser": {
      "main": "core/core",
      "redirects": {
        "montage": "core/core"
      },
      "mappings": {
        "mr": {
          "name": "mr",
          "location": "packages/mr"
        },
        "q": {
          "name": "q",
          "location": "packages/mr/packages/q"
        }
      }
    }
  },
  "dependencies": {
    "collections": "git+ssh://git@github.com:montagejs/collections.git#a58ee3863cd3fee61e0331ac4af87130fe0db5f0",
    "frb": "git+ssh://git@github.com:montagejs/frb.git",
    "mr": "~0.15.3",
    "mousse": "git+ssh://git@github.com:montagejs/mousse.git#910d22ca9993bebf76bd843b6a7b9aaa1d340fef",
    "htmlparser2": "~3.0.5",
    "q-io": "~1.11.0",
    "q": "1.0.0"
  },
  "devDependencies": {
    "montage-testing": "~0.4.1",
    "jshint": "~2.4.4"
  },
  "scripts": {
    "test": "node test/node/node-spec.js",
    "jsdoc": "node ../montagejs.org/build/jsdoc/jsdoc.js -out $PWD/builds/jsdoc/ montage@npm-link"
  },
  "exclude": [
    "test",
    "tools"
  ]
}
